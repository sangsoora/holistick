                <p><%= service.description.gsub(/\n/, '<br/>').html_safe %></p>
              </div>
            </div>
          </div>
        <% end %>
      </div>
      <% if @practitioner.reviews.count > 0 %>
        <div class="practitioner-review">
          <h4><i class="far fa-comment"></i> Reviews</h4>
          <% @practitioner.reviews.each do |review| %>
            <div class="review-box">
